{
  "name": "@universo/metaverse-frt",
  "version": "0.1.0",
  "description": "Frontend UI for Metaverse management in Universo Platformo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
