  },
  "keywords": ["universo", "metaverse", "frontend"],
  "author": "Vladimir Levadnij and Teknokomo",
  "license": "Omsk Open License",
  "dependencies": {
    "react": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/gulp": "4.0.9",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "rimraf": "^5.0.5"
  }
}
