{
  "name": "@universo/space-builder-frt",
  "version": "0.1.0",
  "description": "Frontend UI components for Space Builder (prompt-to-flow) in Universo Platformo",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && gulp",
    "dev": "tsc -w",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "vitest run --config vitest.config.ts"
  },
  "keywords": ["universo", "space-builder", "frontend", "prompt-to-flow"],
  "author": "Vladimir Levadnij and Teknokomo",
  "license": "Omsk Open License",
  "dependencies": {
    "react": "^18.2.0",
    "typescript": "^5.4.5",
    "@universo/auth-frt": "workspace:*"
  },
  "devDependencies": {
    "@types/gulp": "4.0.9",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "rimraf": "^5.0.5",
    "vitest": "^2.1.4"
  }
}
