{
    "name": "@universo/template-mui",
    "version": "0.1.0",
    "description": "Material-UI template implementation for Universo Platformo React",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/index.js"
        },
        "./components/table/FlowListTable": {
            "import": "./dist/esm/components/table/FlowListTable.js",
            "require": "./dist/components/table/FlowListTable.js"
        },
        "./components/dialogs": {
            "import": "./dist/esm/components/dialogs/index.js",
            "require": "./dist/components/dialogs/index.js"
        },
        "./components/dialogs/EntityFormDialog": {
            "import": "./dist/esm/components/dialogs/EntityFormDialog.js",
            "require": "./dist/components/dialogs/EntityFormDialog.js"
        }
    },
    "scripts": {
        "clean": "rimraf dist",
        "build": "pnpm run clean && tsc -p tsconfig.json && tsc -p tsconfig.esm.json && gulp",
        "dev": "tsc -w",
        "lint": "eslint --ext .ts,.tsx,.jsx src/",
        "test": "jest"
    },
    "dependencies": {
        "@mui/material": "^6.1.6",
        "@mui/icons-material": "^6.1.6",
        "@mui/x-charts": "^7.22.2",
        "@mui/x-data-grid": "^7.22.2",
        "@mui/x-data-grid-pro": "^7.22.2",
        "@mui/x-date-pickers": "^7.22.2",
        "@mui/x-date-pickers-pro": "^7.22.2",
        "@mui/x-tree-view": "^7.22.2",
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "dayjs": "^1.11.10",
        "react-i18next": "^15.1.1",
        "i18next": "^23.16.4"
    },
    "devDependencies": {
        "@types/node": "^20.11.17",
        "@types/react": "^18.2.55",
        "eslint": "^8.56.0",
        "gulp": "^4.0.2",
        "rimraf": "^5.0.5",
        "typescript": "^5.4.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/user-event": "^14.5.2",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0"
    },
    "typesVersions": {
        "*": {
            "components/dialogs": [
                "dist/components/dialogs/index.d.ts"
            ],
            "components/dialogs/EntityFormDialog": [
                "dist/components/dialogs/EntityFormDialog.d.ts"
            ],
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "~6.3.0"
    },
    "keywords": [
        "universo",
        "mui",
        "material-ui",
        "template",
        "react"
    ],
    "author": "Universo Platformo Team",
    "license": "MIT"
}
