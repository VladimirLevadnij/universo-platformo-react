{
  "name": "@universo/api-client",
  "version": "0.1.0",
  "description": "TypeScript API client for Universo Platformo",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown && tsc --emitDeclarationOnly",
    "dev": "tsdown --watch",
    "lint": "eslint --ext .ts,.tsx src/",
    "test": "jest"
  },
  "dependencies": {
    "@universo/utils": "workspace:*",
    "axios": "catalog:"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.90.3",
    "@universo/auth-frt": ">=0.1.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "@universo/auth-frt": "workspace:*"
  },
  "keywords": [
    "universo",
    "api",
    "client",
    "typescript"
  ],
  "author": "Universo Platformo Team",
  "license": "MIT"
}
