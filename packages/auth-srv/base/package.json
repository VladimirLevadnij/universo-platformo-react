{
        "name": "@universo/auth-srv",
        "version": "0.1.0",
        "description": "Passport.js + Supabase session toolkit for Universo Platformo",
        "private": true,
        "main": "dist/index.js",
        "module": "dist/index.mjs",
        "types": "dist/index.d.ts",
        "exports": {
                ".": {
                        "types": "./dist/index.d.ts",
                        "import": "./dist/index.mjs",
                        "require": "./dist/index.js"
                }
        },
        "engines": {
                "node": ">=18.15.0 <19 || >=20"
        },
        "scripts": {
                "clean": "rimraf dist",
                "build": "tsdown",
                "dev": "tsdown --watch",
                "lint": "eslint --ext .ts src"
        },
        "dependencies": {
                "@supabase/supabase-js": "^2.45.4",
                "csurf": "^1.11.0",
                "express": "^4.19.2",
                "express-rate-limit": "^7.4.0",
                "express-session": "^1.18.2",
                "passport": "^0.7.0",
                "passport-local": "^1.0.0",
                "zod": "^3.23.8"
        },
        "devDependencies": {
                "@types/csurf": "^1.11.5",
                "@types/express": "^4.17.21",
                "@types/express-session": "^1.18.2",
                "@types/node": "^20.14.9",
                "@types/passport": "^1.0.16",
                "@types/passport-local": "^1.0.38",
                "rimraf": "^5.0.5",
                "tsdown": "catalog:",
                "typescript": "catalog:"
        }
}
