{
    "name": "@universo/metaverses-frt",
    "version": "0.1.0",
    "description": "Frontend module for Metaverses management",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts"
        },
        "./i18n": {
            "types": "./dist/i18n/index.d.ts",
            "import": "./dist/i18n/index.mjs",
            "require": "./dist/i18n/index.js"
        },
        "./pages/*": {
            "default": "./src/pages/*"
        },
        "./package.json": "./package.json"
    },
    "sideEffects": [
        "dist/i18n/index.mjs",
        "dist/i18n/index.js"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "tsdown",
        "dev": "tsdown --watch",
        "lint": "eslint --ext .ts,.tsx,.jsx src/",
        "test": "vitest run --config vitest.config.ts"
    },
    "keywords": [
        "universo",
        "metaverses",
        "frontend"
    ],
    "author": "Vladimir Levadnij and Teknokomo",
    "license": "Omsk Open License",
    "dependencies": {
        "@emotion/react": "catalog:",
        "@emotion/styled": "catalog:",
        "@mui/lab": "5.0.0-alpha.156",
        "@mui/material": "5.15.0",
        "@mui/x-tree-view": "^6.0.0",
        "@tabler/icons-react": "^3.30.0",
        "@universo/template-mui": "workspace:*",
        "react": "catalog:",
        "react-i18next": "catalog:",
        "typescript": "catalog:",
        "uuid": "^9.0.0",
        "axios": "catalog:"
    },
    "peerDependencies": {
        "react": ">=18",
        "react-dom": ">=18",
        "react-router-dom": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "catalog:",
        "@types/react": "catalog:",
        "eslint": "catalog:",
        "rimraf": "^5.0.5",
        "tsdown": "catalog:",
        "vitest": "catalog:",
        "@testing-library/react": "catalog:",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/user-event": "catalog:",
        "jsdom": "catalog:"
    }
}
